{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES5",
    "module": "ES6",
    "outDir": "./dist/js",
    "declaration": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "wwwroot/js/**/*.ts",
    "wwwroot/lib/MyLib/**/*.js"
  ]
}
